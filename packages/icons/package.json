{
	"name": "@frontal/icons",
	"description": "Frontal's icon library",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"default": "./src/index.ts"
		}
	},
	"scripts": {
		"generate": "node scripts/generate-icons.js",
		"lint": "biome check",
		"lint:fix": "biome check --write --unsafe",
		"format": "biome format --write",
		"check": "biome check",
		"check:fix": "biome check --write",
		"type-check": "tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"clean": "git clean -xdf -- node_modules .next .turbo .cache",
		"remove-fill-icons": "node scripts/remove-fill-icons.js",
		"remove-line-suffix": "node scripts/remove-line-suffix.js",
		"test": "bun test"
	},
	"peerDependencies": {
		"react": "^19.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@svgr/core": "^8.1.0",
		"@types/node": "^24.7.0",
		"@types/react": "^19.2.1",
		"jsdom": "^25.0.0",
		"tsx": "^4.7.1",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"icons",
		"svg",
		"ui"
	],
	"engines": {
		"node": ">=18.0.0"
	}
}
