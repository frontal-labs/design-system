{
	"name": "@frontal/next-config",
	"description": "Shared Next.js configuration for Frontal projects",
	"version": "0.0.2",
	"private": true,
	"type": "module",
	"packageManager": "bun@1.3.3",
	"engines": {
		"bun": ">=1.1.0"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./bundle-analyzer": {
			"types": "./dist/bundle-analyzer.d.ts",
			"import": "./dist/bundle-analyzer.js",
			"default": "./dist/bundle-analyzer.js"
		},
		"./keys": {
			"types": "./dist/keys.d.ts",
			"import": "./dist/keys.js",
			"default": "./dist/keys.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"clean": "git clean -xdf -- node_modules .next .cache dist",
		"type-check": "bunx tsc --noEmit --emitDeclarationOnly false",
		"test": "bun test || (echo 'No tests found' && exit 0)"
	},
	"dependencies": {
		"@next/bundle-analyzer": "16.1.1",
		"@t3-oss/env-nextjs": "^0.13.10",
		"next": "^16.1.1",
		"zod": "4.3.5"
	},
	"devDependencies": {
		"@frontal/typescript-config": "workspace:*",
		"@types/minimatch": "^5.1.2",
		"@types/node": "^20.0.0",
		"typescript": "^5.0.0"
	}
}
